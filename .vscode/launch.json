  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ingestor - Classic",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/ingestor",
      "runtimeArgs": ["ingest"],
      "runtimeExecutable": "yarn",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "env": {
        "CREATE_SETS": "true",
        "INGEST_TO_SAAS_SKYLARK": "false",
        "CREATE_ONLY": "true"
      }
    },
    {
      "name": "Debug ingestor - SaaS",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/ingestor",
      "runtimeArgs": ["ingest"],
      "runtimeExecutable": "yarn",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "env": {
        "CREATE_SETS": "true",
        "INGEST_TO_SAAS_SKYLARK": "true"
      }
    }
  ]
}
